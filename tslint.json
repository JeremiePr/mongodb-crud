{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "max-line-length": [
            true,
            200
        ],
        "one-line": false,
        "ordered-imports": false,
        "no-console": false,
        "array-type": false,
        "arrow-parens": false,
        "trailing-comma": false,
        "object-literal-sort-keys": false,
        "interface-name": false,
        "no-var-requires": false
    },
    "rulesDirectory": []
}